    EnableFocus = False
    ColCount = 10
    RowCount = 8
    ColWidth = 32
    RowHeight = 32
    ViewTopLine = 0
    OnGridSelect = DGridSaveItemGridSelect
    OnGridMouseMove = DGridSaveItemGridMouseMove
    OnGridPaint = DGridSaveItemGridPaint
    OnButton = mbLeft
  end
end
