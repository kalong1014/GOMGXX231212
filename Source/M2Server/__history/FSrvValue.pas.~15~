procedure TFrmServerValue.PowerfulblessingoilChange(Sender: TObject);
var
nInteger:Integer;
begin
   nInteger:=Powerfulblessingoil.Position;
   sPowerfulblessingoil.Text:=IntToStr(nInteger);
   g_Powerfulblessingoil:=nInteger;
end;
